menu "GPIO Manager"
    menu "MODULE PARAM"
    ####################################################
    config GPIO_MANAGER_MODULE_NAME
        string "Module Name"
        default "GPIOManager"
        help
            Write the name of module.
    ####################################################
    config GPIO_MANAGER_BRIEF
        string "Module Brief"
        default "System Components"
        help
            Write the brief of module.
    ####################################################
    config GPIO_MANAGER_VERSION
        string "Module Version"
        default "3.0.0"
        help
            Write the version of module.
    ####################################################
    config GPIO_MANAGER_CMD_NAME
        string "Command Name"
        default "{GPIOManager}"
        help
            Write the name of task.
    ####################################################
        config GPIO_MANAGER_RUNNING
        bool "Task Running"
        default y
        help
            Write the name of task.
    ####################################################
    endmenu
endmenu