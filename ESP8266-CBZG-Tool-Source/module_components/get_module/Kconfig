menu "ESP MODULE"
    menu "MODULE PARAM"
    ####################################################
    config ESP_GET_MODULE_MODULE_NAME
        string "Module Name"
        default "NULL"
        help
            Write the name of module.
    ####################################################
    config ESP_GET_MODULE_BRIEF
        string "Command Name"
        default "System Components"
        help
            Write the brief of module.
    ####################################################
    config ESP_GET_MODULE_VERSION
        string "Command Name"
        default "3.0.0"
        help
            Write the version of module.
    ####################################################
    config ESP_GET_MODULE_CMD_NAME
        string "Command Name"
        default "{ESPGETMODULE}"
        help
            Write the name of task.
    ####################################################
        config ESP_GET_MODULE_RUNNING
        bool "Task Running"
        default n
        help
            Write the name of task.
    ####################################################
    endmenu
endmenu